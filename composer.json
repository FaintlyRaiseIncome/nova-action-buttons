{
  "name": "pavloniym/nova-action-buttons",
  "description": "A Laravel Nova field.",
  "keywords": [
    "laravel",
    "nova",
    "actions",
    "buttons"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/pavloniym/nova-action-buttons"
    }
  ],
  "require": {
    "pavloniym/nova-action-buttons": "dev-bugfix",
    "php": "^7.3|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Pavloniym\\ActionButtons\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pavloniym\\ActionButtons\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
